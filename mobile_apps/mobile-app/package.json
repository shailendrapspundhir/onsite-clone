{
  "name": "mobile-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --noEmit",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "setup": "pnpm install && npx pod-install ios",
    "setup_android_emulator": "echo \"Install Android SDK and create an AVD. Set AVD_NAME env and run mobile:run_android.\"",
    "run_android": "react-native run-android",
    "run_ios": "react-native run-ios",
    "build_apk": "cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-screens": "^3.20.0",
    "zustand": "^4.4.0",
    "graphql": "^16.6.0",
    "graphql-request": "^7.4.0",
    "graphql-tag": "^2.12.6",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "react-native-keychain": "^9.1.0",
    "@onsite360/mobile-ui-shared": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/react": "^18.2.21",
    "@types/react-native": "^0.72.6",
    "metro-react-native-babel-preset": "^0.76.7",
    "@babel/core": "^7.23.0",
    "eslint": "^8.50.0",
    "@types/jest": "^29.5.5",
    "vitest": "^1.1.5",
    "@react-native/metro-config": "^0.76.7",
    "@react-native/gradle-plugin": "0.72.6",
    "@react-native/codegen": "0.72.6"
  }
}


